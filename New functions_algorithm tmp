#素数探索
def prime(x):
    if x < 2:  # 2より小さい数値は素数ではない
        print(f"{x} isn't a prime number.")
        return False
    for i in range(2, int(x ** 0.5) + 1):
        if x % i == 0:
            print(f"{x} isn't a prime number.")
            return False
    print(f"{x} is a prime number.")
    return True

